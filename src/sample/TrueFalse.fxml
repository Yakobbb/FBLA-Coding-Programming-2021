<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Background" prefHeight="750.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TrueFalseController">
   <children>
      <Label fx:id="questionText" alignment="CENTER" layoutX="85.0" layoutY="115.0" prefHeight="110.0" prefWidth="730.0" text="Question" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button fx:id="trueButton" layoutX="179.0" layoutY="292.0" mnemonicParsing="false" onAction="#selectTrue" prefHeight="167.0" prefWidth="218.0" text="True" textFill="WHITE">
         <font>
            <Font name="System Bold" size="44.0" />
         </font></Button>
      <Button fx:id="falseButton" layoutX="503.0" layoutY="292.0" mnemonicParsing="false" onAction="#selectFalse" prefHeight="167.0" prefWidth="218.0" text="False" textFill="WHITE">
         <font>
            <Font name="System Bold" size="44.0" />
         </font></Button>
      <ProgressBar fx:id="progressBar" layoutX="126.0" layoutY="609.0" prefHeight="28.0" prefWidth="672.0" progress="0.0" />
      <Label fx:id="questionInstructions" alignment="CENTER" layoutX="298.0" layoutY="38.0" prefHeight="77.0" prefWidth="324.0" text="Select True or False." textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
