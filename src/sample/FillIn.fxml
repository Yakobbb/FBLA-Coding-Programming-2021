<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MenuBackground" prefHeight="750.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FillInController">
   <children>
      <Label fx:id="questionText" alignment="CENTER" layoutX="107.0" layoutY="154.0" prefHeight="110.0" prefWidth="730.0" text="Question" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="16.0" />
         </font></Label>
      <ProgressBar fx:id="progressBar" layoutX="136.0" layoutY="619.0" prefHeight="28.0" prefWidth="672.0" progress="0.0" />
      <TextField fx:id="textField" layoutX="297.0" layoutY="356.0" onKeyPressed="#pressedEnter" prefHeight="38.0" prefWidth="307.0" promptText="Answer" />
      <Label fx:id="questionInstructions" alignment="CENTER" layoutX="243.0" layoutY="39.0" prefHeight="77.0" prefWidth="459.0" text="Fill in the blank, and then press &quot;Enter&quot;." textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
